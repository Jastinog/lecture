{
  "venvPath": "./",
  "venv": ".venv",
  
  "reportArgumentType": false,
  "reportAttributeAccessIssue": false,
  
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "autoSearchPaths": true,
  
  "extraPaths": [],
  
  "ignore": [
    "**/node_modules",
    "**/__pycache__",
    "src/experimental",
    "src/typestubs"
  ],
  
  "strict": []
}
